NEXT_PUBLIC_TESTNET_MAX_FEE_GWEI=150
NEXT_PUBLIC_TESTNET_PRIORITY_FEE_GWEI=1

# Mainnet (optimize for lower costs)
NEXT_PUBLIC_MAINNET_MAX_FEE_GWEI=120
NEXT_PUBLIC_MAINNET_PRIORITY_FEE_GWEI=1
